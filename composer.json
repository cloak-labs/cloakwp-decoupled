{
  "name": "cloakwp/decoupled",
  "description": "Transform WordPress into a headless CMS.",
  "type": "wordpress-plugin",
  "version": "0.0.1",
  "license": "LGPL-3.0-only",
  "require": {
    "cloakwp/block-parser": "^0.0",
    "cloakwp/core": "^0.0",
    "cloakwp/hook-modifiers": "^0.0",
    "cloakwp/virtual-fields": "^0.0",
    "composer/installers": "^2.2",
    "usefulteam/jwt-auth": "^3.0"
  },
  "require-dev": {
    "franzl/studio": "^0.16"
  },
  "autoload": {
    "psr-4": {
      "CloakWP\\": "src"
    },
    "classmap": [
      "vendor/jwt-auth"
    ]
  },
  "extra": {
    "installer-paths": {
      "vendor/{$name}/": [
        "type:wordpress-plugin"
      ]
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "composer/installers": true,
      "franzl/studio": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}