{
  "name": "cloak-labs/wp-hook-modifiers",
  "description": "A package to create WordPress hook modifiers for more granular filtering and actions",
  "version": "0.0.1",
  "license": "LGPL-3.0-only",
  "require": {
    "composer/installers": "^2.2",
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "CloakWP\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "composer/installers": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}